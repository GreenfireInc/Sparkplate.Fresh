{
  "name": "Greenery",
  "description": "A Cryptocurrency Wallet with Accounting Functions",
  "version": "0.9.11",
  "private": true,
  "author": "The Greenery team, a subdivision of Greenfire <Hello@greenfire.io> (https://greenery.finance)",
  "main": "dist/background/index.cjs",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:8080/"
    }
  },
  "scripts": {
    "dev": "npm run build:qrscanner && vite",
    "dev:force": "npm run build:qrscanner && vite --force",
    "dev:qrscanner": "vite --config ./qrscanner/vite.config.qrscanner.js",
    "build": "npm run petra && cross-env NODE_ENV=production npm run build:vite && electron-builder build --config .electron-builder.config.js",
    "build:stage": "npm run petra && cross-env NODE_ENV=staging npm run build:vite && electron-builder build --config .electron-builder.config.js --dir",
    "build:vite": "npm run build:qrscanner && vite build",
    "build:qrscanner": "vite build --config ./qrscanner/vite.config.qrscanner.js && node ./scripts/formatQrScannerHTML.js",
    "lint": "eslint --ext .js,.vue --fix src background",
    "format": "prettier .  --write",
    "petra": "node ./scripts/createPetra.js",
    "postinstall": "electron-builder install-app-deps && patch-package",
    "postuninstall": "electron-builder install-app-deps"
  },
