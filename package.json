{
  "name": "sparkplate-fresh",
  "version": "2025.10.19",
  "main": "dist/background/index.js",
  "description": "A fresh take on the Electron + Vue + Vite boilerplate using Greenfire's Sparkplate and Greenery.",
  "author": "Greenfire <hello@greenfire.io>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "greenfire",
    "sparkplate",
    "greenery",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
