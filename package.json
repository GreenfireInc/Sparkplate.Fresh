    "@vue/runtime-dom": "^3.5.22",
    "autoprefixer": "^10.4.21",
    "buffer": "^6.0.3",
    "electron": "^37.7.0",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.3",
    "sass-embedded": "^1.89.0",
